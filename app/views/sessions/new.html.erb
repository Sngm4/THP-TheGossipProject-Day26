<%= form_tag url_for(action: 'create'), method: "post" do %>
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      <%= label_tag 'E-mail' %>
      <%= email_field_tag 'session_email', nil %>
      <br />
      <br />
      <%= label_tag 'Password' %>
      <%= password_field_tag 'session_password', nil %>
      <br />
      <br />
      <%= submit_tag "identification" %>
    <% end %>